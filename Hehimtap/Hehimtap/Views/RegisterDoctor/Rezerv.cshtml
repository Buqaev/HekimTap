@model List<Hehimtap.Models.Rezerv>
@{
    ViewBag.Title = "Rezerv";
    Layout = "~/Views/Shared/_DoctorLayout.cshtml";
}

@foreach (var x in Model)
{

    <div class="content-wrapper">
        <div class="tab-pane" id="timeline">
            <!-- The timeline -->
            <div class="timeline timeline-inverse">
                <!-- timeline time label -->
                <div class="time-label">
                    <span class="bg-danger">
                        @x.tarix
                    </span>
                </div>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <div>
                    <i class="fas fa-envelope bg-primary"></i>

                    <div class="timeline-item">
                        @*<span class="time"><i class="far fa-clock"></i> 12:05</span>*@

                        <h3 class="timeline-header"><a href="#">@x.User.Ad @x.User.Soyad</a> @x.User.Mail</h3>

                        <div class="timeline-body">
                            @x.Ziyaret.Novu
                        </div>
                        <div class="timeline-footer">
                            <a href="#" class="btn btn-primary btn-sm">Qeydiyyat</a>

                        </div>
                    </div>
                </div>
                <!-- END timeline item -->
                <!-- timeline item -->
                <div>
                    <i class="fas fa-user bg-info"></i>

                    <div class="timeline-item">
                        <span class="time"><i>qiymet</i> @x.Ziyaret.Qiymet $</span>

                        <h3 class="timeline-header border-0">
                            <a href="#">Sarah Young</a> accepted your friend request
                        </h3>
                    </div>
                </div>



                <div>
                    <i class="far fa-clock bg-gray"></i>
                </div>
            </div>
        </div>
        </div>
        }


